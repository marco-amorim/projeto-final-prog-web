<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UsuarioServlet</servlet-name>
  	<display-name>UsuarioServlet</display-name>
  	<description></description>
  	<servlet-class>projetoprogweb.servlets.UsuarioServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>projetoprogweb.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProdutoServlet</servlet-name>
  	<display-name>ProdutoServlet</display-name>
  	<description></description>
  	<servlet-class>projetoprogweb.servlets.ProdutoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UsuarioServlet</servlet-name>
  	<url-pattern>/UsuarioServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProdutoServlet</servlet-name>
  	<url-pattern>/ProdutoServlet</url-pattern>
  </servlet-mapping>
</web-app>
